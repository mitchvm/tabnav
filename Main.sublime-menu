          {
            "caption": "-"
          },
          {
            "caption": "Trim whitespace from selections",
            "command": "tabnav_trim_whitespace_from_selection"
          },
          {
            "caption": "Merge adjacent selections",
            "command": "tabnav_merge_adjacent_selections"
